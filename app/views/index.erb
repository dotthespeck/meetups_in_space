<h1><%= @title %></h1>


<table width="100%">
<tr>
  <th>Name</th>
  <th>Description</th>
  <th>Location</th>
</tr>

<tr>
  <% @Meetups.each do |meetup| %>
<td><a href="/meetup/<%= meetup.id %>"><%= meetup.name %></a></td>
<td><%= meetup.description %></td>
<td><%= meetup.location %></td>
</tr>
<% end %>
</table>
